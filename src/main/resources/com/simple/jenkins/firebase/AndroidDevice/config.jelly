<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">

    <f:entry title="Device model" field="model">
        <f:textbox />
    </f:entry>

    <f:entry title="Android SDK version" field="version">
        <f:textbox />
    </f:entry>

    <f:entry title="Initial orientation" field="orientation">
